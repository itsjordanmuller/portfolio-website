---
import { Image } from "astro:assets";
import AvatarImage from "../../assets/images/Jordan-Muller-Portrait.jpg";
---

<figure class="gradient-img-bg my-0 max-w-xs rounded-2xl p-2 lg:mb-12">
	<Image
		src={AvatarImage}
		alt="Jordan Muller Portrait"
		title="Jordan Muller Portrait"
		class="gradient-bg rounded-2xl p-1"
		loading="eager"
		quality="mid"
	/>
</figure>
